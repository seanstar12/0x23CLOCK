{
  "name": "0x23CLOCK",
  "version": "0.1.1",
  "manifest_version": 2,
  "description": "Pretty Clock",
  "icons": {
    "128": "images/icon.png"
  },
  "background": {
    "page": "background.html"
  },
  "browser_action": { 
    "default_icon": "images/iconPage.png"
  },
  "permissions": [
    "tabs", 
    "background"
  ],
  "web_accessible_resources": [
    "js/*",
    "fonts/*",
    "clock.html"
  ]
}
